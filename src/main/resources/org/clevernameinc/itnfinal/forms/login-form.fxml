<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="198.0" prefWidth="237.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.clevernameinc.itnfinal.forms.LoginForm">
   <children>
      <Label layoutX="14.0" layoutY="48.0" text="Username" />
      <Label layoutX="14.0" layoutY="92.0" text="Password" />
      <TextField fx:id="mUsernameField" layoutX="71.0" layoutY="44.0" />
      <PasswordField fx:id="mPasswordField" layoutX="71.0" layoutY="87.0" />
      <Button layoutX="93.0" layoutY="159.0" mnemonicParsing="false" onAction="#onLoginPressed" text="Login" />
      <Label fx:id="mLoginFailLabel" layoutX="41.0" layoutY="130.0" text="Invalid Username or Password" textFill="RED" visible="false" />
   </children>
</AnchorPane>
